<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yang.dao.CloudAccountMapper">
    <select id="getCloudAccountList" resultType="com.yang.pojo.CloudAccount">
        select * from cmdbv1.cloud_account
    </select>
    <select id="getCloudAccount" parameterType="String" resultType="com.yang.pojo.CloudAccount">
        select * from cmdbv1.cloud_account  where id=#{String}
    </select>
</mapper>
